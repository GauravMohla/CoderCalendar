{
"manifest_version": 2,

  "name": "Coder's Calendar",
  "short_name": "Coder's Calendar",
  "description": "Shows a list of upcoming Programming Contests happeing on a variety of popular platforms.",
  "version": "0.2",

  "permissions": [
    "http://contesttrackerapi.herokuapp.com/",
    "tabs"
  ],
   "icons": { "16": "img/icon32.png",
              "32": "img/icon32.png",
           "48": "img/icon32.png",
          "128": "img/icon32.png" },
  "browser_action": {
    "default_icon": "img/icon32.png",
    "default_popup": "popup.html"
  }
}